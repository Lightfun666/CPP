nint main(){\n    const int cj = 44;\n\n    // error: assign to const is\n    // j = 66;\n\n    // cannot bind non-const lvalue reference of type \n    // 'int&' to an rvalue of type 'int'\n    // int &i = 2 * 2;\n\n    // const reference bind to temporary value(r-value)\n    const int &cir = 2 * 2; \n\n\n    int i = 6;\n    const int &ri0 = i;     // bind to int\n    const int &ri1 = i * 2; // bind to temporary (r-value)\n\n    // error: ri0 is reference of const\n    // can not used to change value\n    // ri0 = 7;\n\n    double d = 3.1415;\n    const int &ri2 = d; // temporary value is created\n\n\n\n    return 0;\n}\n\n1.2. pointer and const\n#include \nusing std::cout;\nusing std::endl;\nint main() {\n    const double pi = 3.1415;\n\n    // error: const double* to double*\n    // double *ptr = &pi; \n\n    const double *cptr = &pi;\n\n    // error: assign const double value\n    // *cptr = 42;\n\n    double dval = 3.1415;\n    cptr = &dval;\n\n    int ivalue = 0;\n    int *const curErr = &ivalue;\n    const double *const dp = &pi;\n    // dp is const pointer to const double\n\n    return 0;\n}\n\n1.3. constexpr\n#include \nusing std::cout;\nusing std::endl;\nint main() {\n\n    // constexpr value can not be changed\n    // and evaluate in compile time\n    const int i = 123;\n    constexpr int j = i + 123; // j is const int\n\n    constexpr const int *p0 = &i; // const pointer to const int\n\n    int k = 6;\n    constexpr int *p1 = &k; // const pointer to int\n\n    return 0;\n}\n\n"},"pages/Ch02/Ch2-5.html":{"url":"pages/Ch02/Ch2-5.html","title":"5 types","keywords":"","body":"1. types1.1. variable1. types\n1.1. variable\n#include\nusing std::cout;\nusing std::endl;\n\nint main(){\n    int i = 1;\n    cout \n2\n"},"pages/Ch02/Ch2-6.html":{"url":"pages/Ch02/Ch2-6.html","title":"6 data structure","keywords":"","body":"1. data structure1.1. variable1. data structure\n1.1. variable\n#include\nusing std::cout;\nusing std::endl;\n\nint main(){\n    int i = 1;\n    c